{
    "functions": {
        "*.php": {
            "runtime":  "vercel-php@0.7.0"
        }
    },
    "routes": [
        {
            "src":  "/(css|assets|sql)/(.*)",
            "dest":  "/$1/$2"
        },
        {
            "src": "/(.*\\.php)",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index. php"
        }
    ]
}

